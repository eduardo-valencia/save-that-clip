{
  "name": "react-chrome-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.0",
    "@sentry/react": "^7.57.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "sonner": "^2.0.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "popup-test": "jest --config ./popup/jest.config.ts",
    "popup-test:watch": "npm run popup-test -- --watch",
    "base-lint-ts": "tsc --noEmit",
    "popup-lint-ts": "npm run base-lint-ts -- --project ./popup/tsconfig.json",
    "main-test": "jest --config ./main/jest.config.ts",
    "main-test:watch": "npm run main-test -- --watch",
    "main-lint-ts": "npm run base-lint-ts -- --project ./main/tsconfig.json",
    "test": "run-p main-test popup-test",
    "lint-ts": "run-p main-lint-ts popup-lint-ts",
    "lint": "npm run lint-ts",
    "dev": "npx gulp dev",
    "build": "npx cross-env NODE_ENV=production npx gulp build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@sentry/webpack-plugin": "^2.4.0",
    "@types/gulp": "^4.0.11",
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2",
    "@types/webpack-stream": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "chrome-types": "^0.1.209",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-stream": "^7.0.0"
  },
  "engines": {
    "node": ">=14 <15"
  }
}
